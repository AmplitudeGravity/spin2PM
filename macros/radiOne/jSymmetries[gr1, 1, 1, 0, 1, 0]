{{j[gr1, (n1_)?Positive, (n2_)?Positive, (n3_)?NonPositive, (n4_)?Positive, 
    (n5_)?NonPositive] :> Expand[j[gr1, n1, n2, n3, n4, n5]]}, 
 {j[gr1, (n1_)?Positive, (n2_)?Positive, (n3_)?NonPositive, (n4_)?Positive, 
    (n5_)?NonPositive] :> Expand[j[gr1, n1, n2, 0, n4, n5]/
     (-j[gr1, 0, 0, -1, 0, 0])^n3]}, 
